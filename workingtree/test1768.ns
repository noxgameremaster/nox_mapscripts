    Effect("TELEPORT", GetObjectX(unit), GetObjectY(unit), 0.0, 0.0);
    Effect("SMOKE_BLAST", GetObjectX(unit), GetObjectY(unit), 0.0, 0.0);
    DeleteObjectTimer(CreateObjectAt("BlueRain", destX, destY), 25);
    Enchant(unit, "ENCHANT_LIGHT", 5.0);
}

