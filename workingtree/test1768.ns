void drawStrLevelUp(int arg_0, string name)
{
	int count, i;
	float pos_x, pos_y;

	if (!count)
	{
		pos_x = LocationX(1);
		pos_y = LocationY(1);
	}
	for (i = 1 ; i > 0 && count < 558 ; i <<= 1)
	{
		if (i & arg_0)
			CreateObject(name, 1);
		if (count % 45 == 44)
            TeleportLocationVector(1, -88.0, 2.0);
		else
            TeleportLocationVector(1, 2.0, 0.0);
		count ++;
	}
	if (count >= 558)
	{
		count = 0;
		TeleportLocation(1, pos_x, pos_y);
	}
}

