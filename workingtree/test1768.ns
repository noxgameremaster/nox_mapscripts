    if (HasClass(other, "MONSTERGENERATOR") && CurrentHealth(other))
    {
        flag = GetDirection(self);
        GeneratorAllign();
        if (flag)
        {
            if (flag ^ 1)
            {
                x = 152;
                y = 208;
            }
            else
            {
                x = 143;
                y = 199;
            }
        }
        else
        {
            x = 134;
            y = 190;
        }
        for (i = 0 ; i < 4 ; i ++)
            WallOpen(Wall(x + i, y + i));
    }
}

