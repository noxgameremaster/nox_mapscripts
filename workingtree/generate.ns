float IntToFloat(int x)
{
    int i;
    int pos = x;
    float result = 0.0;
    if (pos < 0)
        pos = -pos;
    for (i = 0; i < 32; i+=1)
    {
        if (pos & 0x80000000)
            result += 1.0;
        pos = pos << 1;
        if (i != 31)
            result *= 2.0;
    }
    if (x < 0)
        return -result;
    else
        return result;
}

