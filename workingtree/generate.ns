int CheckNealyPlayer(int unit)
{
    float temp = 9999.0, r;
    int i, res = -1;

    for (i = 9 ; i >= 0 ; i --)
    {
        if (CurrentHealth(player[i]) && CheckVisibleUnitToUnit(player[i], unit))
        {
            r = Distance(GetObjectX(unit), GetObjectY(unit), GetObjectX(player[i]), GetObjectY(player[i]));
            if (r < temp)
            {
                temp = r;
                res = i;
            }
        }
    }
    return res;
}

